<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<package name="user-action" namespace="/nsfw" extends="base-default">
		<action name="user_*" class="my.it.nsfw.user.action.UserAction" method="{1}">
			<result name="{1}">/WEB-INF/jsp/nsfw/user/{1}.jsp</result>
			<result name="list" type="redirectAction">
				<param name="actionName">user_listUI.action</param>
				<param name="user.name">${name}</param>
				<param name="pageNo">${pageNo}</param>
				<param name="encode">true</param>
			</result>
		</action>
	</package>	
	
</struts>
